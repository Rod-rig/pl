{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "jsRules": {
    "quotemark": false
  },
  "rules": {
    "quotemark": false,
    "jsx-no-multiline-js": false,
    "no-var-requires": false,
    "no-submodule-imports": ["material-ui"]
  },
  "rulesDirectory": []
}