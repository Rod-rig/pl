{
  "name": "predictor",
  "version": "1.0.0",
  "main": "webpack.config.js",
  "license": "MIT",
  "scripts": {
    "build": "webpack -p",
    "build:w": "webpack --watch",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:js": "tslint -t stylish 'public/src/**/*.{ts*,js}'",
    "lint:css": "stylelint \"public/src/**/*.css\"",
    "mongo": "mongod",
    "server": "nodemon server.js --ignore public/",
    "server:dev": "webpack-dev-server",
    "start": "concurrently \"npm run build:w\" \"npm run mongo\" \"npm run server:dev\"",
    "pretest": "npm run lint",
    "test": "jest --config package.json",
    "test:coverage": "jest --config package.json --coverage",
    "test:ci": "npm run test:coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Rod-rig/pl"
  },
  "keywords": [
    "markup",
    "front-end"
  ],
  "author": "Vadim Cherniak <cherniakvadim08@gmail.com>",
  "bugs": {
    "url": ""
  },
  "homepage": "https://rod-rig.github.io/pl/",
  "browserslist": [
    "> 1%",
    "ie >= 11",
    "last 3 versions"
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/jest.config.js",
    "moduleNameMapper": {
      "^.+\\.(css)$": "identity-obj-proxy"
    },
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.4.1",
    "body-parser": "^1.17.2",
    "concurrently": "^3.5.1",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.4",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.2.2",
    "mongodb": "^3.0.2",
    "nodemon": "^1.12.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-csso": "^3.0.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.6",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.20.1",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "ts-jest": "^22.0.3",
    "tslint": "^5.9.1",
    "tslint-react": "^3.4.0",
    "typescript": "^2.6.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.9.7",
    "webpack-spritesmith": "^0.4.0"
  },
  "dependencies": {
    "@types/enzyme": "^3.1.8",
    "@types/enzyme-adapter-react-16": "^1.0.1",
    "@types/jest": "^22.1.2",
    "@types/react": "^16.0.35",
    "@types/react-dom": "^16.0.3",
    "@types/react-test-renderer": "^16.0.0",
    "axios": "^0.18.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "material-ui": "^1.0.0-beta.33",
    "material-ui-icons": "^1.0.0-beta.17",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-test-renderer": "^16.2.0"
  }
}
