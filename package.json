{
  "name": "predictor",
  "version": "0.4.19",
  "main": "public/src/index.tsx",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack.prod.config.js",
    "build:w": "webpack --config webpack.dev.config.js --watch",
    "lint": "npm run lint:js",
    "lint:js": "tslint -t stylish -p tsconfig.json",
    "pretest": "npm run lint",
    "server": "node server/app",
    "server:dev": "nodemon server/app",
    "start": "npm run server",
    "test": "jest --config package.json",
    "test:ci": "npm run test:coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "test:coverage": "jest --config package.json --coverage",
    "cron": "node server/crons"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Rod-rig/pl"
  },
  "keywords": [
    "markup",
    "front-end"
  ],
  "author": "Vadim Cherniak <cherniakvadim08@gmail.com>",
  "bugs": {
    "url": "https://github.com/Rod-rig/pl/issues"
  },
  "homepage": "https://predict0r.herokuapp.com/",
  "browserslist": [
    "last 2 versions"
  ],
  "nodemonConfig": {
    "ignore": [
      "public/*"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "public/src/components/**/*.{ts,tsx}",
      "public/src/helpers/**/*.{ts,tsx}",
      "public/src/stores/**/*.{ts,tsx}",
      "!public/**/index.{ts,tsx}"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest.config.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "prettier": {
    "trailingComma": "all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write **/*.{js,jsx,ts,tsx,json,css,md}",
      "git add"
    ]
  },
  "devDependencies": {
    "coveralls": "^3.0.3",
    "husky": "^1.3.1",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.6",
    "nodemon": "^1.19.0",
    "prettier": "^1.17.0",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^23.10.4",
    "ts-loader": "5.3.3",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.4.5",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-middleware": "^3.6.2"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.1",
    "@types/enzyme": "^3.9.1",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.12",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.3",
    "@types/react-test-renderer": "^16.8.1",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "dotenv": "^6.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "express": "^4.16.4",
    "express-session": "^1.16.1",
    "mobx": "^5.9.4",
    "mobx-react": "^5.3.6",
    "mongoose": "^5.5.7",
    "morgan": "^1.9.1",
    "query-string": "^6.5.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.8.6",
    "uuid": "^3.3.2",
    "validator": "^10.11.0"
  }
}
