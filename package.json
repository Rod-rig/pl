{
  "name": "predictor",
  "version": "0.2.30",
  "main": "public/src/index.tsx",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack.prod.config.js",
    "build:w": "webpack --config webpack.dev.config.js --watch",
    "lint": "npm run lint:js",
    "lint:js": "tslint -t stylish 'public/src/**/*.{tsx,ts,js}'",
    "pretest": "npm run lint",
    "server": "node server/app.js",
    "server:dev": "nodemon server/app.js",
    "start": "yarn run server",
    "test": "jest --config package.json",
    "test:ci": "npm run test:coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "test:coverage": "jest --config package.json --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Rod-rig/pl"
  },
  "keywords": [
    "markup",
    "front-end"
  ],
  "author": "Vadim Cherniak <cherniakvadim08@gmail.com>",
  "bugs": {
    "url": "https://github.com/Rod-rig/pl/issues"
  },
  "homepage": "https://rod-rig.github.io/pl/",
  "browserslist": [
    "last 2 versions"
  ],
  "jest": {
    "collectCoverageFrom": [
      "public/src/components/**/*.{ts,tsx}",
      "public/src/helpers/**/*.{ts,tsx}",
      "public/src/stores/**/*.{ts,tsx}",
      "!public/**/index.{ts,tsx}"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/jest.config.js",
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "jest": "^23.5.0",
    "nodemon": "^1.17.4",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.5.0",
    "tslint": "^5.10.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.1",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.0.8",
    "webpack-dev-middleware": "^3.1.2"
  },
  "dependencies": {
    "@material-ui/core": "^1.5.0",
    "@material-ui/icons": "^2.0.2",
    "@types/enzyme": "^3.1.13",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.1.3",
    "@types/query-string": "^6.1.0",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "@types/react-router-dom": "^4.2.6",
    "@types/react-test-renderer": "^16.0.2",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.0.0",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.5",
    "mongoose": "^5.2.9",
    "morgan": "^1.9.0",
    "query-string": "^6.1.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.4.2"
  }
}
