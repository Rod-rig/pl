{
  "name": "predictor",
  "version": "1.0.0",
  "main": "public/src/index.tsx",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack.prod.config.js",
    "build:w": "webpack --config webpack.dev.config.js --watch",
    "lint": "npm run lint:js",
    "lint:js": "tslint -t stylish 'public/src/**/*.{tsx,ts,js}'",
    "mongo": "mongod",
    "pretest": "npm run lint",
    "server": "nodemon server.js --ignore public/",
    "server:dev": "webpack-dev-server --config webpack.dev.config.js",
    "start": "concurrently \"npm run build:w\" \"npm run mongo\" \"npm run server:dev\"",
    "test": "jest --config package.json",
    "test:ci": "npm run test:coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "test:coverage": "jest --config package.json --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Rod-rig/pl"
  },
  "keywords": [
    "markup",
    "front-end"
  ],
  "author": "Vadim Cherniak <cherniakvadim08@gmail.com>",
  "bugs": {
    "url": "https://github.com/Rod-rig/pl/issues"
  },
  "homepage": "https://rod-rig.github.io/pl/",
  "browserslist": [
    "> 1%",
    "ie >= 11",
    "last 3 versions"
  ],
  "jest": {
    "collectCoverageFrom": [
      "public/src/components/**/*.{ts,tsx}"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/jest.config.js",
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "body-parser": "^1.18.2",
    "compression-webpack-plugin": "^1.1.11",
    "concurrently": "^3.5.1",
    "coveralls": "^3.0.0",
    "express": "^4.16.3",
    "jest": "^22.4.3",
    "mongodb": "^3.0.5",
    "nodemon": "^1.17.3",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.20.3",
    "ts-jest": "^22.4.2",
    "ts-loader": "^4.1.0",
    "tslint": "^5.9.1",
    "tslint-react": "^3.5.1",
    "typescript": "^2.8.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1",
    "webpack-spritesmith": "^0.4.1"
  },
  "dependencies": {
    "@types/enzyme": "^3.1.9",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^22.2.2",
    "@types/react": "^16.3.5",
    "@types/react-dom": "^16.0.4",
    "@types/react-router-dom": "^4.2.6",
    "@types/react-test-renderer": "^16.0.1",
    "axios": "^0.18.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "material-ui": "^1.0.0-beta.43",
    "material-ui-icons": "^1.0.0-beta.17",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.3.1"
  }
}
